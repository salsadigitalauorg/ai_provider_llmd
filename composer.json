{
  "name": "drupal/ai_provider_llmd",
  "description": "AI Provider for RedHat LLM-d distributed inference framework",
  "type": "drupal-module",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Claude Code",
      "email": "noreply@anthropic.com"
    }
  ],
  "require": {
    "drupal/core": "^10.2 || ^11",
    "drupal/ai": "*",
    "drupal/key": "*"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.7",
    "phpstan/phpstan": "^1.10",
    "phpmd/phpmd": "^2.13",
    "drupal/coder": "^8.3",
    "phpstan/extension-installer": "^1.3",
    "mglaman/phpstan-drupal": "^1.2"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "extra": {
    "drupal": {
      "version": "1.0.0-dev",
      "datestamp": "1751770000"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "phpstan/extension-installer": true
    }
  }
}
