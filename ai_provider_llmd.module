<?php

/**
 * @file
 * LLM-d AI Provider module.
 */

declare(strict_types=1);

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function ai_provider_llmd_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    case 'help.page.ai_provider_llmd':
      return '<p>' . t('This module provides integration with RedHat LLM-d distributed inference framework for the AI module.') . '</p>';
  }
}
